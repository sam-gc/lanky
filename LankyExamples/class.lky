Dog = class() -> self {
    self.build_ = func(name) {
        self.name = name;
        self.weight = 40;
    };

    self.equals_ = func(other) {
        _prt self.name + " " + other.name;
        _prt (self.name == other.name);
        ret (self.name == other.name & self.weight == other.weight);
    };

    self.bark = func() {
        _prt self.name + " : Bark!";
    };
};

moki = Dog("Moki");

moki.bark();
_prt moki.weight;

_prt "----------------";

moki.bark();

