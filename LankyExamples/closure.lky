make_adder = func() {
    count = 0;
    obj = 0 + 1;
    obj.add = func() {
        count = count + 1;
    };
    
    obj.get_count = func() {
        ret count;
    };

    _prt obj;
    ret obj;
};

adder = make_adder();
_prt adder.add;
ad2 = make_adder();
adder.add();
