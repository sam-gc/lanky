nested = [ ["a", "b", "c", "d"], [1, 2, 3, 4, 5], ["x", "y", "z"] ];

nested.forEach(func(iarr, idx) {
    Io.putln("In array " + idx);
    iarr.forEach(func(val) {
        Io.putln("    " + val);
    });
    Io.putln("");
});

loop(i = 0; i < 10000000; i = i + 1) {
    nested[2] = i;
    Io.putln(i);
}   
