Io.putln("These are a few of my favorite things:");

things = Array();

line = "...";

loop(line != "") {
    line = Io.prompt("> ");
    if(line != "") {
        things.append(line);
    }
}

of = Io.fopen("things.txt", "w");

Io.putln("");
Io.putln("       Favorite Things");
Io.putln("==========>>>oOo<<<==========");

things.forEach(func(val, idx) {
    Io.putln((idx + 1) + ": " + val);
    of.putln(val);
});

of.close();
